{
  "name": "@coderabbit-website/front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "gcp-build": "next build",
    "codegen": "graphql-codegen --config codegen.yml",
    "dev": "next dev",
    "prelint": "tsc --noEmit",
    "lint": "eslint --max-warnings=0 . ",
    "prelint:fix": "tsc --noEmit && prettier --write .",
    "lint:fix": "eslint --fix .",
    "performance": "yarn build && npx @lhci/cli collect",
    "start": "next start -p 8080",
    "postinstall": "patch-package",
    "email:dev": "bash -c 'template=${1:-template_1}; template=${template%.mjml}; echo \"Watching template: $template.mjml\"; mjml email/mjml/$template.mjml -o email/html/$template.html && open email/html/$template.html && nodemon --watch \"email/mjml/$template.mjml\" --ext mjml --ignore email/html/ --exec \"mjml email/mjml/$template.mjml -o email/html/$template.html\"' --",
    "email:build": "bash -c 'template=${1:-template_1}; template=${template%.mjml}; mjml email/mjml/$template.mjml -o email/html/$template.html' --"
  },
  "dependencies": {
    "@dotlottie/react-player": "^1.6.19",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@next/third-parties": "^14.2.15",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-switch": "^1.1.2",
    "@sentry/nextjs": "^8.51.0",
    "@tailwindcss/typography": "^0.5.15",
    "@types/qs": "^6.9.15",
    "@types/react-lottie": "^1.2.10",
    "@unly/universal-language-detector": "^2.0.3",
    "@vercel/flags": "^2.6.3",
    "@vercel/functions": "^1.5.2",
    "@wojtekmaj/react-hooks": "^1.22.0",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "eslint-config-custom": "*",
    "framer-motion": "^11.5.4",
    "graphql": "^16.10.0",
    "graphql-request": "^6.1.0",
    "intersection-observer-debugger": "^0.1.1",
    "js-base64": "^3.7.5",
    "lucide-react": "^0.468.0",
    "markdown-to-jsx": "^7.5.0",
    "next": "^14.2.5",
    "next-intl": "4.0",
    "next-secure-headers": "^2.2.0",
    "patch-package": "^8.0.0",
    "postcss": "^8.4.41",
    "posthog-js": "^1.249.0",
    "posthog-node": "^4.18.0",
    "postinstall-postinstall": "^2.1.0",
    "qs": "^6.13.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hot-toast": "^2.5.2",
    "react-lottie": "^1.2.4",
    "react-pdf": "^9.2.1",
    "react-tweet": "^3.2.1",
    "rss": "^1.2.2",
    "sanitize-html": "^2.11.0",
    "sharp": "^0.33.5",
    "slug": "^8.2.3",
    "swiper": "^11.2.1",
    "tailwind-merge": "^2.5.5",
    "tailwindcss": "^3.4.10",
    "tailwindcss-animate": "^1.0.7",
    "universal-cookie": "^7.2.2",
    "uuidv7": "^1.0.2",
    "validator": "^13.11.0",
